CREATE TABLE course 
(
c_code CHAR(8) PRIMARY KEY,
c_name VARCHAR(100),
c_credit INT,
c_type VARCHAR(15),
status CHAR(1),
admin_id varchar(10),
l_id CHAR(6),
d_id CHAR (3)

);
