CREATE TABLE final_mark
(
TG_number varchar(15),
c_code char(8),
final decimal,
ca decimal,
total decimal,
grade varchar(2),
FOREIGN KEY(TG_number) REFERENCES student(TG_number),
FOREIGN KEY(c_code) REFERENCES course(c_code),
FOREIGN KEY(grade) REFERENCES grade(value)
);
​ 

