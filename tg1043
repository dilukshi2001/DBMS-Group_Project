CREATE TABLE user
(
u_id varchar(10) PRIMARY KEY,
Nic CHAR(12),
email VARCHAR(100),
fname VARCHAR(50),
lname VARCHAR(50),
contact CHAR(11),
gender VARCHAR(7),
role VARCHAR(7)
);


