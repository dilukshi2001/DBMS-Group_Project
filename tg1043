CREATE DATABASE group11;

USE group11;


CREATE TABLE student
(
TG_number VARCHAR(15) PRIMARY KEY,
sur_name VARCHAR(20),
lastName VARCHAR(20),
Address VARCHAR(100),
Email varchar(100),
status VARCHAR(10),
dob DATE,
gender VARCHAR(10),
d_id char(3),
mentor CHAR(6)

);



